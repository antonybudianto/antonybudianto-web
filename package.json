{
  "name": "antonybudianto-web",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.3.0",
    "prettier": "1.13.7",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@cra-express/core": "^2.2.5",
    "bootswatch": "4.1.1",
    "cra-universal": "^3.0.0",
    "dotenv": "^6.0.0",
    "firebase": "^5.2.0",
    "font-awesome": "^4.7.0",
    "loadable-components": "2.2.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1"
  },
  "scripts": {
    "crau:b":
      "microbundle crau.config.es.js -o=crau.config.js -f=cjs --sourcemap=false --compress=false",
    "crau:babel": "babel crau.config.es.js -o crau.config.js",
    "prettier": "prettier --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "copy:server": "cp -a ./dist ./functions/crau-dist",
    "copy": "npm run copy:server && rm build/index.html",
    "start": "react-scripts start",
    "crau:start": "cra-universal start",
    "build": "react-scripts build",
    "crau:build": "cra-universal build",
    "crau:clean": "cra-universal clean",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "firebase deploy",
    "clean": "cra-universal clean && rimraf functions/crau-dist"
  }
}
